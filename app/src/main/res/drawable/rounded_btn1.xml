<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <corners
                        android:radius="140dp"
                        />
                    <gradient
                        android:startColor="#E1D100"
                        android:endColor="#DB7200"
                        />
                </shape>
            </item>
            <item>
                <inset android:inset="1dp">
                    <shape android:shape="rectangle">
                        <corners
                            android:radius="140dp"
                            />
                        <solid android:color="#FFFFFF"/>
                    </shape>
                </inset>
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <corners
                        android:radius="140dp"
                        />
                    <gradient
                        android:startColor="#E1D100"
                        android:endColor="#DB7200"
                        />
                </shape>
            </item>
            <item>
                <inset android:inset="1dp">
                    <shape android:shape="rectangle">
                        <corners
                            android:radius="140dp"
                            />
                        <solid android:color="#1D2923"/>
                    </shape>
                </inset>
            </item>
        </layer-list>
    </item>
</selector>
