<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <inset>
        <shape android:shape="rectangle" android:dither="true">
            <corners
                android:radius="140dp"
                />
<!--            <solid-->
<!--                android:color="#FACB01"-->
<!--                />-->
            <gradient
                android:startColor="#DC8701"
                android:centerColor="#FACB01"
                android:endColor="#DC8701"
                android:angle="135"
                android:type="linear"
                />
        </shape>
        </inset>
        <rotate android:toDegrees="95"/>
    </item>
    <item android:gravity="center">
        <inset android:inset="1dp">
            <shape android:shape="rectangle" android:dither="true">
                <corners
                    android:radius="140dp"
                    />
                <gradient
                    android:startColor="#FACB01"
                    android:endColor="#DC8701"
                    android:angle="0"
                    />
            </shape>
        </inset>
    </item>
