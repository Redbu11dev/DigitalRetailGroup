<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="140dp"
                />
            <gradient
                android:startColor="#FACB01"
                android:endColor="#DC8701"
                android:angle="-90"
                />
        </shape>
    </item>
    <item>
        <inset android:inset="2px">
            <shape android:shape="rectangle">
                <corners
                    android:radius="140dp"
                    />
                <gradient
                    android:startColor="#FACB01"
                    android:endColor="#DB7200"
                    />
            </shape>
        </inset>
    </item>
</layer-list>
