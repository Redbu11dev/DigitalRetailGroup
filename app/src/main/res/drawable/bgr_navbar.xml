<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="140dp"
                />
            <gradient
                android:startColor="#323E38"
                android:endColor="#00323E38"
                android:angle="-90"
                />
        </shape>
    </item>
    <item>
        <inset android:inset="1dp">
            <shape android:shape="rectangle">
                <corners
                    android:radius="140dp"
                    />
                <solid android:color="#1D2923"/>
            </shape>
        </inset>
    </item>
</layer-list>
